        <!-- Start Navigation -->
        <nav
          class="collapse navbar-collapse bs-navbar-collapse navbar-right"
          role="navigation"
        >
          <ul class="nav navbar-nav">
            <li>
              <a href="/">홈</a>
            </li>
            <li>
              <a href="/#gallery">대표 공원</a>
